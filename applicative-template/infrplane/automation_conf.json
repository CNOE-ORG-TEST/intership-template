{
  "infr_components": {
      "aws-cni": {
          "enabled": "${{values.enableAwsCni}}",
          "enableNetworkPolicy": "false"
      },
      "coredns": {
          "enabled": "${{values.enableCoredns}}",
          "resources": {
              "limit_cpu": "300m",
              "limit_ram": "170Mi",
              "request_cpu": "100m",
              "request_ram": "70Mi"
          }
      },
      "cluster_autoscaler": {
          "enabled": "${{values.enableClusterAutoscaler}}",
          "parameters": {
              "expander": "least-waste",
              "scale_down_time": "10"
          },
          "resources": {
              "limit_cpu": "250m",
              "limit_ram": "2Gi",
              "request_cpu": "250m",
              "request_ram": "2Gi"
          }
      },
      "metric-server": {
          "enabled": "${{values.enableMetricServer}}",
          "resources": {
              "limit_cpu": "300m",
              "limit_ram": "500Mi",
              "request_cpu": "100m",
              "request_ram": "200Mi"
          }
      }
  },
  "infrpanel_version": "1.30.4"
}
