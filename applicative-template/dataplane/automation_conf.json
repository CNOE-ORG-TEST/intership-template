{
  "nodegroups":[
    {
      "datapanel_cloudformation_name":"${{values.nodeGroupName}}-DataPanel-Stack",
      "update":"true",
      "only_major":"false",
      "cloudformation_parameters":{
        "backup_parameter":"no_backup",
        "tag_nodegroup_name":"${{values.nodeGroupName}}",
        "subnet_az":"",
        "node_min":"0",
        "node_max":"0",
        "node_desidered":"0",
        "max_pods":"44",
        "ondemand_base_capacity":"0",
        "node_volume":{
          "size":"${{values.volumeSize}}",
          "type":"${{values.volumeType}}",
          "iops":"${{values.volumeIOPS}}"
        }
      },
      "cloudformation_options":{
        "is_spot":"${{values.isSpot}}",
        "is_graviton":"${{values.isGravitation}}",
        "is_gpu":"${{values.isGpu}}",
        "taints":"${{values.taints}}",
        "labels":"${{values.labels}}",
        "machine_size":[
          "m6i.2xlarge",
          "m5.2xlarge"
        ],
        "instances_in_parallel":"4",
        "pause_time":"4"
      }
    }
  ],
  "datapanel_version":"${{values.eksVersion}}"
}